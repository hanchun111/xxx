<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://www.bosssoft.com.cn/schema/uis"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.bosssoft.com.cn/schema/uis"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:import namespace="http://www.w3.org/XML/1998/namespace" />
	<xsd:annotation>
		<xsd:documentation>
			<![CDATA[博思软件ui框架的应用程序上下文支持配置元素]]>
		</xsd:documentation>
	</xsd:annotation>
	<!-- 定义uipage -->
	<xsd:element name="Page">
	<xsd:annotation>
		<xsd:documentation>
			<![CDATA[页面模型控件]]>
		</xsd:documentation>
	</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="items" type="typePageItems">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[页面模型对象 子项可包含{Panel,Import}]]>
					</xsd:documentation>
				</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="id" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation>ID</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>控件名称</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="templatePath" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[模板路径 例如 platform/test/testindex]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="template" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[数据库中模板编码，在使用数据库中定义的模板时，填写该属性]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="scopeKey" type="xsd:string">
			</xsd:attribute>
			<xsd:attribute name="js" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[js脚本路径]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="jsId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[js对象id]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="onPageLoad" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[页面加载完成后的事件函数，可以额外的手动做一些初始化工作。
							函数包含一个参数，是后台Controller方法中里返回的model中设置到UIConst.UIPAGEMODEL的值
							例如：webapp/resources/north/amodel/a.js
							  define(["app/app-base","app/app-pagebase",], function(Base,PageBase) {
							  	FunctionModel = PageBase.extend({
							  	
							  	});
							  	
							  	return new FunctionModel();
							  
							  })
							xml: <Page id="function_index" templatePath="platform/func/function/function_index" jsId="a" js="resources/north/amodel/a" onPageLoad="a.init"> </page>]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Panel" type="typePanel">
	<xsd:annotation>
		<xsd:documentation>
			<![CDATA[面板控件]]>
		</xsd:documentation>
	</xsd:annotation>
	</xsd:element>
	<xsd:element name="Tree" type="typeTree">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[树控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- 表单 -->
	<xsd:element name="Form" type="typeUIForm">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[表单控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Row" type="typeRow">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FormItem" type="typeFormItem">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[表单元素控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- 表单结束 -->
	<xsd:element name="Grid" type="typeGrid">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[网格控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="XGrid" type="typeXGrid">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[网格控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ButtonArea" type="typeUIButtonArea">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[按钮区控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Button" type="typeUIButton">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[按钮控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Menu" type="typeMenu">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[菜单控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MenuItem" type="typeUIMenuItem">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[菜单元素控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TreeGrid" type="typeTreeGrid">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[树形网格控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Upload" type="typeUpload">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[上传组件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Query" type="typeUIQuery">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[查询控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="QueryItem" type="typeUIQueryItem">
	<xsd:annotation>
			<xsd:documentation>
				<![CDATA[查询项]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="Import" type="typeUIImport">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Tabs" type="typeTabs">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[页签控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TabPanel" type="typeTabPanel">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[页签面板控件]]>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<!-- 自定义类型定义开始 -->
	<xsd:complexType name="typePageItems">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[页面元素对象， 可包含类型 {Panel,Import}]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="1" maxOccurs="unbounded">
			<xsd:element ref="Panel" maxOccurs="unbounded" minOccurs="0"></xsd:element>
			<xsd:element ref="Import" maxOccurs="unbounded"
				minOccurs="0"></xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="typePanelItems">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[面板下包含元素{Panel,Tree,Form,Grid,
						XGrid,TreeGrid,ButtonArea,Upload,
						Query,Import,Tabs}]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="1" maxOccurs="unbounded">
			<xsd:element ref="Panel" maxOccurs="unbounded" minOccurs="0"></xsd:element>
			<xsd:element ref="Tree" maxOccurs="unbounded" minOccurs="0"></xsd:element>
			<xsd:element ref="Form" maxOccurs="unbounded" minOccurs="0"></xsd:element>
			<xsd:element ref="Grid" maxOccurs="unbounded" minOccurs="0"></xsd:element>
			<xsd:element ref="XGrid" maxOccurs="unbounded" minOccurs="0"></xsd:element>
			<xsd:element ref="TreeGrid" maxOccurs="unbounded"
				minOccurs="0"></xsd:element>
			<xsd:element ref="ButtonArea" maxOccurs="unbounded"
				minOccurs="0"></xsd:element>
			<xsd:element ref="Upload" maxOccurs="unbounded"
				minOccurs="0"></xsd:element>
			<xsd:element ref="Query" maxOccurs="unbounded" minOccurs="0"></xsd:element>
			<xsd:element ref="Import" maxOccurs="unbounded"
				minOccurs="0"></xsd:element>
			<xsd:element ref="Tabs" maxOccurs="unbounded" minOccurs="0"></xsd:element>
		</xsd:choice>
	</xsd:complexType>

	<xsd:complexType name="typeRowItems">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[行对象下所包含元素]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="1" maxOccurs="unbounded">
			<xsd:element ref="FormItem" maxOccurs="unbounded"
				minOccurs="0"></xsd:element>
		</xsd:choice>
	</xsd:complexType>

	<xsd:complexType name="typeFormItems">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[表单元素下所包含元素  {Row}]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="1" maxOccurs="unbounded">
			<xsd:element ref="Row" maxOccurs="unbounded" minOccurs="0"></xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="typeTabsItems">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[页签所包含元素]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="1" maxOccurs="unbounded">
			<xsd:element ref="TabPanel" maxOccurs="unbounded"
				minOccurs="0"></xsd:element>
		</xsd:choice>
	</xsd:complexType>

	<xsd:complexType name="typeTabPanel">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[页签面板所包含元素]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="items" type="typePanelItems">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[页签面板对象   可包含子项{Panel,Tree,Form,Grid,
								XGrid,TreeGrid,ButtonArea,Upload,
								Query,Import,Tabs}]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>

		<xsd:attribute name="active" type="xsd:string">
		</xsd:attribute>
		<xsd:attribute name="lazy" type="xsd:boolean">
		</xsd:attribute>
		<xsd:attributeGroup ref="attrPanel"></xsd:attributeGroup>

	</xsd:complexType>

	<xsd:complexType name="typeTabs">
		<xsd:sequence maxOccurs="unbounded" minOccurs="0">
			<xsd:element name="items" type="typeTabsItems">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[页签下包含元素{}]]>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[ID]]>
					</xsd:documentation>
				</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="hideHeader" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[隐藏头部信息]]>
					</xsd:documentation>
				</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="afterSwitchTab" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[切换页签之后]]>
					</xsd:documentation>
				</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="beforeSwitchTab" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<![CDATA[切换页签之前]]>
					</xsd:documentation>
				</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="typePanel">
		<xsd:sequence>
			<xsd:element name="items" type="typePanelItems">
			<xsd:annotation>
				<xsd:documentation>
					<![CDATA[面板对象  子项可包含{Panel,Tree,Form,Grid,XGrid,
							TreeGrid,ButtonArea,Upload,Query,Import,Tabs}]]>
				</xsd:documentation>
			</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="attrPanel"></xsd:attributeGroup>
		<xsd:attributeGroup ref="attrFormDesign"></xsd:attributeGroup>
	</xsd:complexType>

	<xsd:complexType name="typeTree">
		<!-- <xsd:sequence>去除事件 <xsd:element name="events" minOccurs="0" maxOccurs="100"> 
			<xsd:complexType> <xsd:sequence> <xsd:element name="Event" type="typeEvent" 
			minOccurs="1"></xsd:element> </xsd:sequence> </xsd:complexType> </xsd:element> 
			</xsd:sequence> -->
		<xsd:attributeGroup ref="attrTree"></xsd:attributeGroup>
		<xsd:attributeGroup ref="attrFormDesign"></xsd:attributeGroup>

	</xsd:complexType>
	<!-- 表单类型 -->
	<xsd:complexType name="typeUIForm">
		<xsd:choice>
			<xsd:element name="FormGroup" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FormItem" type="typeFormItem"
							minOccurs="0" maxOccurs="100">
						</xsd:element>

					</xsd:sequence>
					<xsd:sequence>
						<xsd:element name="items" type="typeFormItems">
							<xsd:annotation>
								<xsd:documentation>
									<![CDATA[表单对象 子项可包含{attrFormItem,attrFormDesign]]>
								</xsd:documentation>
							</xsd:annotation>	
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute name="id" type="xsd:string" use="required">
						<xsd:annotation>
							<xsd:documentation>ID</xsd:documentation>
						</xsd:annotation>	
					</xsd:attribute>
					<xsd:attribute name="name" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>名称</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="style" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>固定样式信息</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="FormItem" type="typeFormItem"
				minOccurs="0" maxOccurs="unbounded"></xsd:element>
			<xsd:sequence>
				<xsd:element name="items" type="typeFormItems"></xsd:element>
			</xsd:sequence>
		</xsd:choice>
		<xsd:attributeGroup ref="attrForm"></xsd:attributeGroup>
		<xsd:attributeGroup ref="attrFormDesign"></xsd:attributeGroup>
	</xsd:complexType>

	<xsd:complexType name="typeFormItem">
		<xsd:annotation>
			<xsd:documentation>json格式，用于过滤数据字典项
				{
				'nameKey':'aaa', //字典项中名字里包含aaa的
				'codeKey':'01', //字典项中编码包含01的
				'excludes':['001','002','003']
				//排除code=001,002,003的数据
				}
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="events" minOccurs="0" maxOccurs="15">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Event" type="typeEvent" minOccurs="0"
							maxOccurs="15"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="attrFormItem"></xsd:attributeGroup>
		<xsd:attributeGroup ref="attrFormDesign"></xsd:attributeGroup>
	</xsd:complexType>

	<xsd:complexType name="typeRow">
		<xsd:sequence>
			<xsd:element name="items" type="typeRowItems"></xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="attrFormDesign"></xsd:attributeGroup>
	</xsd:complexType>

	<xsd:complexType name="typeGrid">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[网格对象，   可包含类型{attrGrid,attrFormDesign}]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice maxOccurs="unbounded" minOccurs="0">
			<xsd:element name="Column" type="typeGridColumn"
				minOccurs="0" maxOccurs="100"></xsd:element>
			<xsd:element name="ButtonArea" type="typeUIButtonArea"
				minOccurs="0"></xsd:element>
			<xsd:element name="GroupHeaders" type="typeGroupHeaders"
				minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="events" minOccurs="0" maxOccurs="100">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:sequence>
							<xsd:element name="Event" type="typeEvent" minOccurs="0"
								maxOccurs="15"></xsd:element>
						</xsd:sequence>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:choice>
		<xsd:attributeGroup ref="attrGrid"></xsd:attributeGroup>
		<xsd:attributeGroup ref="attrFormDesign"></xsd:attributeGroup>
	</xsd:complexType>
	<xsd:complexType name="typeGridColumn">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[网格列对象，   可包含类型{attrGridColumn}]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- <xsd:element name="events" minOccurs="0"> <xsd:complexType> <xsd:sequence> 
				<xsd:element name="Event" type="typeEvent" minOccurs="0" maxOccurs="15"></xsd:element> 
				</xsd:sequence> </xsd:complexType> </xsd:element> -->
			<xsd:element name="buttons" minOccurs="0" maxOccurs="15">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Button" type="typeUIButton"
							minOccurs="0" maxOccurs="15"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="attrGridColumn"></xsd:attributeGroup>

	</xsd:complexType>
	<xsd:complexType name="typeGroupHeader">
		<xsd:sequence>
			<xsd:element name="GroupHeaders" type="typeGroupHeaders"
				minOccurs="0" maxOccurs="100">
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="attrGroupHeader"></xsd:attributeGroup>

	</xsd:complexType>
	<xsd:complexType name="typeGroupHeaders">
		<xsd:sequence>
			<xsd:element name="GroupHeader" type="typeGroupHeader"
				minOccurs="0" maxOccurs="100">
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="useColSpanStyle" type="xsd:string"></xsd:attribute>

	</xsd:complexType>

	<xsd:complexType name="typeXGrid">
		<xsd:sequence>
			<xsd:element name="toolbar" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Button" type="typeUIButton"
							maxOccurs="unbounded" minOccurs="0">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="frozenColumns" minOccurs="0"
				maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="XGridColumn" type="typeXGridColumn"
							maxOccurs="unbounded" minOccurs="0">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="columns" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="XGridColumn" type="typeXGridColumn"
							maxOccurs="unbounded" minOccurs="0">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="frozenColumnsRight" minOccurs="0"
				maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="XGridColumn" type="typeXGridColumn"
							maxOccurs="unbounded" minOccurs="0">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="events" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Event" type="typeEvent" minOccurs="0"
							maxOccurs="15">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
					<![CDATA[ID]]>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="autoLoad" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>
					<![CDATA[是否自动加载数据]]>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="checkbox" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>
					<![CDATA[是否使用checkbox]]>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="columnResizable" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>
					<![CDATA[网格是否可以调整列宽]]>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="editable" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>
					<![CDATA[网格是否可编辑]]>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fitColumns">
			<xsd:annotation>
				<xsd:documentation>
					<![CDATA[设置自动列宽模式]]>
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="N"></xsd:enumeration>
					<xsd:enumeration value="ES"></xsd:enumeration>
					<xsd:enumeration value="E"></xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="follow">
			<xsd:annotation>
				<xsd:documentation>
					<![CDATA[跟随滚动]]>
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="none"></xsd:enumeration>
					<xsd:enumeration value="header"></xsd:enumeration>
					<xsd:enumeration value="footer"></xsd:enumeration>
					<xsd:enumeration value="all"></xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="group" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>JSON格式

					group.show 是否显示分组头
					group.items 分组信息
					group.items.field 分组的字段
					group.items.order 排序方式可选值：
					ace: 升序
					desc：降序
					group.template 主视图模板
					group.summary 合计值选项
					group.summary.items 合计的字段
					group.summary.items.field 合计的字段
					group.summary.items.type 合计类型可选值：
					sum: 合计值
					avg：平均值
					min：最小值
					max：最大值
					count：计数值
					group.summary.items.value
					要输出的值
					group.summary.method 计算的函数，如果函数为空，则使用分组数据进行替换模板
					group.summary.template 主视图模板
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="headerCustom" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>自定义面板的jquery表达式
					组件根据 $A(headerCustom) 取得面板 eg.
					#query
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="height" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>网格的高度
					1、高度默认为空：撑满整个父容器
					2、当值为push时，高度随着数据的变化而变化
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="columnManager" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>是否允许进行列的隐藏/显示操作</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="markChange" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>是否对编辑过的数据进行标记</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="mergeColumns" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>mergeColumns</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="multiSort" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>是否允许多列排序</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="pageList" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>可选择设定的每页结果数

