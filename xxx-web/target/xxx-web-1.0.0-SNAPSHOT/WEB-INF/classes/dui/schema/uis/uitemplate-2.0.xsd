				<xsd:documentation>分页的位置</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="none"></xsd:enumeration>
					<xsd:enumeration value="up"></xsd:enumeration>
					<xsd:enumeration value="down"></xsd:enumeration>
					<xsd:enumeration value="all"></xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="pageSize" type="xsd:int">
			<xsd:annotation>
				<xsd:documentation>每页默认的结果数，默认10</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="queryParams" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>查询参数
					JSON格式
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="radiobox" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>是否显示单选框</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="remoteSort" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>是否远程排序</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="rownumbers" type="xsd:string">
